<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    width="700"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <footerconfig
                jcr:primaryType="cq:Widget"
                title="Footer Configuration"
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <isfourcolumns
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Select to display 4 columns in the footer, deselect for 5 columns"
                        fieldLabel="Display 4 column footer?"
                        name="./isfourcolumns"
                        type="checkbox"
                        xtype="selection"/>
                    <copyrighttext
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Text to display as the copyright disclaimer. Use [current-year] as a placeholder to be replaced with the actual year"
                        fieldLabel="Copyright"
                        name="./copyrighttext"
                        xtype="textfield"/>
                </items>
            </footerconfig>
            <icon1
                jcr:primaryType="cq:Widget"
                title="Icon 1"
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <icon1icon
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Path of the icon 1 image"
                        fieldLabel="Icon 1 Path"
                        name="./icon1icon"
                        xtype="pathfield"/>
                    <icon1link
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Link for icon 1"
                        fieldLabel="Icon 1 Link"
                        name="./icon1link"
                        xtype="pathfield"/>
                    <icon1alttext
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Text to use as Alt text for icon 1"
                        fieldLabel="Icon 1 Alt Text"
                        name="./icon1alttext"
                        xtype="textfield"/>
                    <icon1interstitial
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Select the interstitial window to display"
                        fieldLabel="Interstitial"
                        name="./icon1interstitial"
                        options="/services/article-module-interstitial?page=$PATH"
                        type="select"
                        xtype="selection"/>
                    <icon1onclick
                        jcr:primaryType="cq:Widget"
                        fieldDescription="JavaScript code to execute when the icon is clicked"
                        fieldLabel="Icon 1 onClick"
                        name="./icon1onclick"
                        xtype="textarea"/>
                </items>
            </icon1>
