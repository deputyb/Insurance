<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <image
                jcr:primaryType="cq:Widget"
                path="/apps/aetna/components/global/content/sidebarImage/dialog/items/items/image.infinity.json"
                xtype="cqinclude"/>
            <imageconfigs
                jcr:primaryType="cq:Widget"
                title="Image Config"
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <resTypeImage
                        jcr:primaryType="cq:Widget"
                        ignoreData="true"
                        name="./image/sling:resourceType"
                        value="foundation/components/image"
                        xtype="hidden"/>
                    <imageConfigs
                        jcr:primaryType="cq:Widget"
                        title="Image configuration"
                        xtype="fieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <imageURL
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Image URL"
                                name="./imageURL"
                                width="250"
                                xtype="pathfield"/>
                        </items>
                    </imageConfigs>
                </items>
            </imageconfigs>
        </items>
    </items>
</jcr:root>
