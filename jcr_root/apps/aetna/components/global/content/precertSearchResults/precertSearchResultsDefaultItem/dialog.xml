<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    height="400"
    title="Precert Search Results Item"
    width="650"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <configuration
                jcr:primaryType="cq:Panel"
                title="Configuration">
                <items jcr:primaryType="cq:WidgetCollection">
                    <resultid
                        jcr:primaryType="cq:Widget"
                        ignoreData="true"
                        name="./resultid"
                        value="default"
                        xtype="hidden"/>
                    <text
                        jcr:primaryType="cq:Widget"
                        allowBlank="false"
                        externalStyleSheets="[/common/css/common-clientlibs/css/styles.css]"
                        fieldDescription="Text to include as the result to display"
                        fieldLabel="Text"
                        name="./text"
                        xtype="richtext">
                        <rtePlugins
                            jcr:primaryType="cq:Widget"
                            path="/apps/aetna/components/global/content/articleModule/dialog/items/items/tab1/items/text/rtePlugins.infinity.json"
                            xtype="cqinclude"/>
                    </text>
                </items>
            </configuration>
        </items>
    </items>
</jcr:root>
