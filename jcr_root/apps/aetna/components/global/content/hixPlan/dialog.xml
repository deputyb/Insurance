<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="HIX Plan Content"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <configuration
                jcr:primaryType="cq:Panel"
                title="Configs">
                <items jcr:primaryType="cq:WidgetCollection">
                    <statePlan
                        jcr:primaryType="cq:Widget"
                        allowBlank="false"
                        fieldDescription="State where the plans belong to"
                        fieldLabel="State"
                        name="./statePlan"
                        xtype="textfield"/>
                    <year
                        jcr:primaryType="cq:Widget"
                        allowBlank="false"
                        fieldDescription="Number with plan's year"
                        fieldLabel="Plan Year"
                        name="./year"
                        xtype="numberfield"/>
                    <planName
                        jcr:primaryType="cq:Widget"
                        allowBlank="false"
                        fieldDescription="Plan name to search on xml file to show pdf links"
                        fieldLabel="Plan Name"
                        name="./planName"
                        xtype="textfield"/>
                </items>
            </configuration>
        </items>
    </items>
</jcr:root>
